{
  "name": "webrtc-signal-http",
  "version": "1.8.3",
  "description": "opinionated webrtc signal provider using http as a protocol",
  "main": "dist/lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "bin": {
    "webrtc-signal-http": "./dist"
  },
  "files": [
    "lib/**/*",
    "package-lock.json",
    "index.ts",
    "tsconfig.json",
    "LICENSE",
    "dist/**/*"
  ],
  "scripts": {
    "test": "ts-mocha test/**/*.ts",
    "prepare": "npm run build",
    "build": "tsc -p .",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "node dist/index.js"
  },
  "types": "index.d.ts",
  "keywords": [
    "webrtc",
    "signal"
  ],
  "author": "Ben Greenier",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-bunyan-logger": "^1.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.2",
    "@types/express-bunyan-logger": "^1.3.1",
    "@types/mocha": "^5.0.0",
    "mocha": "^5.0.0",
    "supertest": "^3.0.0",
    "ts-mocha": "^6.0.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  }
}
